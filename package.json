{
  "name": "my-board-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/smart-dev.js 3000",
    "dev:safe": "node scripts/port-finder.js 3000 && next dev --turbopack --port 3000",
    "dev:auto": "./scripts/dev-server.sh",
    "dev:check": "node scripts/port-finder.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'Port 3000 is free'",
    "predev": "npm run kill-port"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "@types/zxcvbn": "^4.4.5",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.0",
    "next": "15.4.5",
    "next-auth": "^5.0.0-beta.29",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^4.2.8",
    "resend": "^6.0.1",
    "styled-jsx": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^4.0.15",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "mongodb": "^6.18.0",
    "mongodb-memory-server": "^10.2.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.54.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
