{
  "name": "my-board-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "prebuild": "tsc --noEmit",
    "dev": "node server.js",
    "dev:next": "node scripts/smart-dev.js 3000",
    "dev:safe": "node scripts/port-finder.js 3000 && next dev --turbopack --port 3000",
    "dev:auto": "./scripts/dev-server.sh",
    "dev:check": "node scripts/port-finder.js",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "precommit": "lint-staged",
    "check": "npm run lint && npm run format:check && npm run test",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:security": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:security": "playwright test tests/e2e/auth/security.spec.ts",
    "test:e2e:rate-limit": "playwright test tests/e2e/auth/rate-limit.spec.ts",
    "test:e2e:resend": "playwright test tests/e2e/auth/email-resend.spec.ts",
    "test:e2e:improvements": "playwright test tests/e2e/improvements/",
    "test:e2e:realtime": "playwright test tests/e2e/improvements/realtime-features.spec.ts",
    "test:e2e:moderation": "playwright test tests/e2e/improvements/moderation-features.spec.ts",
    "test:e2e:search": "playwright test tests/e2e/improvements/search-features.spec.ts",
    "test:e2e:integrated": "playwright test tests/e2e/improvements/integrated-features.spec.ts",
    "test:e2e:improvements:headed": "playwright test tests/e2e/improvements/ --headed",
    "test:e2e:ci": "npm run build && playwright test",
    "test:e2e:tags:toggles": "playwright test tests/e2e/tags.toggles.all-variants.spec.ts",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'Port 3000 is free'",
    "predev": "npm run kill-port",
    "clean": "rm -rf .next node_modules/.cache .turbo",
    "reset": "npm run clean && npm install",
    "test:resend": "node scripts/test-comprehensive-resend.js",
    "test:resend:simple": "USE_SIMPLE_API=true node scripts/test-comprehensive-resend.js",
    "setup:db": "node scripts/setup-indexes.js",
    "migrate:follows": "npx ts-node src/lib/db/migrations/add-follow-indexes.ts",
    "backfill:tags": "npx ts-node --project tsconfig.scripts.json scripts/backfill-tags.ts",
    "test:follow": "npx ts-node scripts/test-follow-feature.ts",
    "test:follow:js": "node scripts/test-follow-feature.js",
    "test:follow:jest": "jest src/__tests__/models/follow.test.ts --verbose",
    "fix:auto": "node scripts/auto-fix-and-test.js",
    "deploy:check": "node scripts/deploy-checklist.js",
    "deploy:env": "node scripts/generate-production-env.js",
    "deploy:test": "npm run lint && npm run typecheck && npm run test:unit",
    "deploy:preview": "vercel",
    "deploy:prod": "npm run deploy:check && vercel --prod",
    "postinstall": "node scripts/post-install.js 2>/dev/null || true",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "bundle:analyze": "ANALYZE=true npm run build",
    "prepare": "husky"
  },
  "overrides": {
    "jose": "4.15.9",
    "tmp": "^0.2.5"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.10.2",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/dompurify": "^3.0.5",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.17.20",
    "@types/zxcvbn": "^4.4.5",
    "axios-cookiejar-support": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "date-fns": "^4.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^11.1.0",
    "mongoose": "^8.17.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prop-types": "15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^4.2.8",
    "react-transition-group": "4.4.5",
    "resend": "^6.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-jsx": "^5.1.7",
    "swr": "^2.3.6",
    "tough-cookie": "^5.1.2",
    "uuid": "^11.1.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.8",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.1.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/prop-types": "15.7.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "4.4.6",
    "@types/uuid": "^10.0.0",
    "artillery": "^2.0.24",
    "async-retry": "^1.3.3",
    "axe-playwright": "^2.1.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "husky": "^9.1.0",
    "ioredis": "^5.7.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.1.2",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.5.2",
    "mongodb": "^6.18.0",
    "mongodb-memory-server": "^10.2.0",
    "msw": "^2.10.5",
    "node-fetch": "^2.7.0",
    "playwright": "^1.55.0",
    "puppeteer": "^24.16.1",
    "redis": "^5.8.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
